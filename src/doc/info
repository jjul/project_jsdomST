** mysql table(BOARD)	
	
CREATE TABLE BOARD (
    NO INT(10) NOT NULL PRIMARY KEY AUTO_INCREMENT,
    NAME VARCHAR(100) DEFAULT '',
    SUBJECT VARCHAR(100) DEFAULT '',
    CONTENT VARCHAR(100) DEFAULT '',
    REGDATE DATE
);